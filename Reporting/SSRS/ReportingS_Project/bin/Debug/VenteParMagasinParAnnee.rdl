  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EMODE</DataSourceName>
        <CommandText>SELECT        O.SHOP_NAME, C.YEAR, SUM(S.AMOUNT_SOLD) AS total_amount
FROM            OUTLET_LOOKUP AS O INNER JOIN
                         SHOP_FACTS2 AS S ON O.SHOP_CODE = S.SHOP_CODE INNER JOIN
                         CALENDAR_YEAR_LOOKUP AS C ON C.WEEK_KEY = S.WEEK_KEY
GROUP BY O.SHOP_NAME, C.YEAR
ORDER BY C.YEAR, O.SHOP_NAME</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SHOP_NAME">
          <DataField>SHOP_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YEAR">
          <DataField>YEAR</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="total_amount">
          <DataField>total_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
