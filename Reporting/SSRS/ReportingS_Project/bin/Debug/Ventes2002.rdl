  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EMODE</DataSourceName>
        <CommandText>SELECT        o.SHOP_NAME, SUM(s.AMOUNT_SOLD) AS Montant
FROM            OUTLET_LOOKUP AS o INNER JOIN
                         SHOP_FACTS2 AS s ON o.SHOP_CODE = s.SHOP_CODE INNER JOIN
                         CALENDAR_YEAR_LOOKUP AS c ON s.WEEK_KEY = c.WEEK_KEY
WHERE        (c.YEAR = 2002)
GROUP BY o.SHOP_NAME</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SHOP_NAME">
          <DataField>SHOP_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Montant">
          <DataField>Montant</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
