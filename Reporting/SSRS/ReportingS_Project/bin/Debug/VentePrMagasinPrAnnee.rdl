          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="total_amount">
          <DataField>total_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
